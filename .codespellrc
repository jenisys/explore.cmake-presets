[codespell]
skip = */.git,*/build,*/build.*/
quiet-level = 3
# builtin = clear,rare,en-GB_to_en-US
ignore-words-list = filetest,JE


# -- ENABLED OPTIONS:
count =
check-filenames =
check-hidden =

